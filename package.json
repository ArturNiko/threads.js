{
  "name": "@a4turp/threads.js",
  "version": "1.4.4",
  "author": "Artur Papikian",
  "license": "MIT",
  "description": "Light and easy web-tool for accessing multiple threads via web workers.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "comment//": "publish: pnpm build --onSuccess 'echo 1' for later automations...",
  "scripts": {
    "build": "tsup src/index.ts --format esm, cjs --dts --clean",
    "watch": "pnpm build --watch",
    "dev": "pnpm build && vite",
    "test": "vitest run tests/module.test.js",
    "deploy": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArturNiko/threads.js.git"
  },
  "bugs": {
    "url": "https://github.com/ArturNiko/threads.js/issues"
  },
  "devDependencies": {
    "happy-dom": "latest",
    "tsup": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vitest": "latest"
  },
  "keywords": [
    "multithreading",
    "multithread",
    "multicore",
    "workers",
    "pools",
    "typescript",
    "ts",
    "javascript",
    "js"
  ],
  "packageManager": "pnpm@8.13.1+sha256.9e5f62ce5f2b7d4ceb3c2848f41cf0b33032c24d683c7088b53f62b1885fb246"
}
